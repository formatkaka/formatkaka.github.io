{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    // "typeRoots": ["./node_modules/phaser/types"],
    // "types": ["Phaser"],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/react/*" : ["src/components/react/ui/*"],
      "@/utils/*": ["src/utils/*"],
      "@/*" : ["src/*"],
    }
  }
}